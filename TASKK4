DECLARE num : INTEGER
INPUT num
WHILE(num<=0 OR num>12) DO
  OUTPUT "INVALID INPUT, ENTER AGAIN:"
  INPUT num
ENDWHILE
IF num == 1 THEN
  OUTPUT "JANUARY"
ELSE IF num == 2 THEN 
  OUTPUT "FEBURARY"
ELSE IF num == 3 THEN 
  OUTPUT "MARCH"
ELSE IF num == 4 THEN 
  OUTPUT "APRIL"
ELSE IF num == 5 THEN 
  OUTPUT "MAY"
ELSE IF num == 6 THEN 
  OUTPUT "JUNE"
ELSE IF num == 7 THEN 
  OUTPUT "JULY"
ELSE IF num == 8 THEN 
  OUTPUT "AUGUST"
ELSE IF num == 9 THEN 
  OUTPUT "SEPTEMBER"
ELSE IF num == 10 THEN 
  OUTPUT "OCTOBER"
ELSE IF num == 11 THEN 
  OUTPUT "NOVEMBER"
ELSE
  OUTPUT "DECEMBER"
ENDIF
